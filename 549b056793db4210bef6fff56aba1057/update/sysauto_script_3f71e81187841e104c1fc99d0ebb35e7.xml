        <script><![CDATA[gs.info('Kicking off weekly on-call schedule');
var ocu = new x_snc_on_call_sc_0.OnCallUtils();

var rotation_id = ocu.getRotation(new GlideDateTime());//new GlideRecord(ocu.getConstants().TABLE_ROTATIONS);

var rotation = new GlideRecord(ocu.getConstants().TABLE_ROTATIONS);
if(rotation.get(rotation_id)){
	var tms = rotation.getValue('assignees');
	gs.info(['team members',tms]);
	var users_arr = [];
	var temp_arr = tms.split(',');
	for(u in temp_arr){
		let u_id = ocu.getUserFromMember(temp_arr[u]);
		users_arr.push(u_id);

	}
	var users = users_arr.join(',');
	gs.info(users);
	gs.eventQueue('x_snc_on_call_sc_0.rotation.started', rotation, users);
} else {
	gs.error('No rotation found for this week, aborting execution.');
}

]]></script>
