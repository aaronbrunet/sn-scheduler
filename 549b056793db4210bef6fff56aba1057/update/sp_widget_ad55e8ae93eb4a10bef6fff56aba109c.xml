        <name>On Call Schedule - Go To Rotation</name>
        <option_schema>[{"name":"title","section":"Presentation","label":"Title","type":"string"}]</option_schema>
        <public>false</public>
        <roles/>
        <script><![CDATA[(function() {
  var ocu = new x_snc_on_call_sc_0.OnCallUtils();
  var constants = ocu.getConstants();

  var shift_page = $sp.getParameter('table') == constants.TABLE_SHIFTS;
  if(shift_page){
	data.shift_page = shift_page;

	var shift = $sp.getRecord();

	if(shift){
		//console.log(JSON.stringify(shift));
		data.rotation_id = shift.rotation.getValue('sys_id');
	}
	return;
  }
  var rotation_page = $sp.getParameter('table') == constants.TABLE_ROTATIONS;
 	if(rotation_page){
		data.rotation_page = rotation_page;
		data.rotation_id = $sp.getParameter('sys_id');
  }
})();]]></script>
