        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var ocu = new x_snc_on_call_sc_0.OnCallUtils();
	var constants = ocu.getConstants();
	var shifts = new GlideRecord(constants.TABLE_SHIFTS);
	shifts.addQuery('rotation',current.sys_id);
	shifts.query();
	while(shifts.next()){
		//setWorkflow(false);
		shifts.setValue('from',current.getValue('from'));
		shifts.setValue('to',current.getValue('to'));
		shifts.update();
	}

