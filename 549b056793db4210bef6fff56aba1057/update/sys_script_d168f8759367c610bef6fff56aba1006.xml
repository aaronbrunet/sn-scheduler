        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var ocu = new x_snc_on_call_sc_0.OnCallUtils();
	var rotation_id = current.rotation;
	var assignees = ocu.getAssigneesByRotation(rotation_id);

	var rotation = new GlideRecord(ocu.constants.TABLE_ROTATIONS);
	rotation.get(rotation_id);

	if(rotation){
		gs.info(`Old assignees value: ${rotation.getValue('assignees')}`);
		gs.info(`New assignees value: ${assignees}`);
		rotation.setValue('assignees',assignees);
		rotation.update();
		//gs.info('test - no update()');
	}




})(current, previous);]]></script>
